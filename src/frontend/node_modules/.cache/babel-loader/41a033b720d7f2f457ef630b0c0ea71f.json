OGIN', userData);\r\n        // console.log(response.headers);\r\n        console.log('완료');\r\n        this.$router.push('/main');\r\n        this.initForm();\r\n      } catch (error) {\r\n        // 에러 핸들링할 코드\r\n        console.log(error.response.data);\r\n        this.logMessage = error.response.data;\r\n      } finally {\r\n        this.initForm();\r\n      }\r\n    },\r\n    initForm() {\r\n      this.username = '';\r\n      this.password = '';\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/*.btn {*/\r\n/*  color: white;*/\r\n/*}*/\r\n</style>\r\n"],"sourceRoot":"src/components"}]}